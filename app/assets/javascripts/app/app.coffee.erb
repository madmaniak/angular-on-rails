angular.module('App', [
  'Common',
  <%= AngularTemplate.find_dependencies @pathname.dirname %>
])

.config( ($locationProvider, $routeProvider) ->
  $locationProvider.html5Mode true
  $routeProvider.otherwise redirectTo: '/not-found'
)

.run( (access) ->
  access.watch()
)

.controller('AppCtrl', -> )
